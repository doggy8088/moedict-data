  "heteronyms": [
    {
      "bopomofo": "ㄏㄨㄟˋ　ㄧˋ",
      "definitions": [
        {
          "def": "聚合眾人商議。",
          "quote": [
            "《後漢書．卷一四．宗室四王三侯傳．齊武王縯傳》：「諸將會議立劉氏，以從人望。」",
            "《五代史平話．梁史．卷上》：「僖宗使宰相會議。」"
          ],
          "synonyms": "集會,聚會"
        },
        {
          "def": "有組織的商議事情的集會。",
          "example": [
            "如：「訓導會議」、「校務會議」。"
          ]
        }
      ],
      "pinyin": "huì yì"
    }
  ],
  "title": "會議"
}