  "heteronyms": [
    {
      "bopomofo": "ㄧㄡˇ　ㄕㄥ",
      "definitions": [
        {
          "def": "泛指一切生物。",
          "quote": [
            "《文選．陸機．豪士賦序》：「且好榮惡辱，有生之所大期。」",
            "《文選．袁宏．三國名臣序贊》：「夫萬歲一期，有生之通塗。」"
          ]
        },
        {
          "def": "生來、一生。",
          "example": [
            "如：「這是我有生以來，最難忘的一件事。」"
          ]
        }
      ],
      "pinyin": "yǒu shēng"
    }
  ],
  "title": "有生"
}