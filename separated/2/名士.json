  "heteronyms": [
    {
      "bopomofo": "ㄇㄧㄥˊ　ㄕˋ",
      "definitions": [
        {
          "def": "有名的人士。",
          "quote": [
            "《三國志．卷六．魏書．袁紹傳》：「議郎何顒等，皆名士也。」",
            "《文明小史．第二二回》：「楊觀察是當今名士。」"
          ],
          "synonyms": "名流﹑名人﹑聞人"
        },
        {
          "def": "名望高而不在位的人。",
          "quote": [
            "《禮記．月令》：「開府庫，出幣帛，周天下，勉諸侯，聘名士，禮賢者。」",
            "《儒林外史．第一三回》：「無奈嘉興的朋友都知道公孫是個做詩的名士。」"
          ]
        },
        {
          "def": "尚高論、倜儻而不務小節的人士，如魏晉時代尚清談之士。"
        }
      ],
      "pinyin": "míng shì"
    }
  ],
  "title": "名士"
}