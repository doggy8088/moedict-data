  "heteronyms": [
    {
      "bopomofo": "ㄏㄨㄟˋ　ㄐㄧㄢˋ",
      "definitions": [
        {
          "def": "進見。",
          "quote": [
            "《後漢書．卷四一．第五倫傳》：「從王朝京師，隨官屬得會見，帝問以政事，倫因此酬對政道，帝大悅。」"
          ],
          "synonyms": "接見　2.會晤"
        },
        {
          "def": "與人見面。《儒林外史．第四一回》：「今幸會見表叔，失敬了。」也作「會面」。"
        },
        {
          "def": "即將見到。",
          "quote": [
            "元．沈禧〈一枝花．瑤臺上品仙套〉：「會見懸魚袋，行看佩虎符。」"
          ]
        }
      ],
      "pinyin": "huì jiàn"
    }
  ],
  "title": "會見"
}