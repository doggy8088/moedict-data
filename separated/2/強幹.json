  "heteronyms": [
    {
      "bopomofo": "ㄑㄧㄤˊ　ㄍㄢˋ",
      "definitions": [
        {
          "def": "主幹強壯。比喻事物的根本堅強穩固。",
          "quote": [
            "《文選．班固．西都賦》：「蓋以強幹弱枝，隆上都而觀萬國也。」"
          ]
        },
        {
          "def": "強壯能幹。",
          "quote": [
            "北齊．顏之推《顏氏家訓．涉務》：「軍旅之臣，取其斷決有謀，強幹習事　。」"
          ]
        }
      ],
      "pinyin": "qiáng gàn"
    },
    {
      "bopomofo": "ㄑㄧㄤˇ　ㄍㄢˋ",
      "definitions": [
        {
          "def": "不計成敗、不怕艱難地硬做。",
          "example": [
            "如：「作生意，有時候即使虧本，也要強幹下去。」"
          ]
        }
      ],
      "pinyin": "qiǎng gàn"
    }
  ],
  "title": "強幹"
}