  "heteronyms": [
    {
      "bopomofo": "ㄔㄨㄟ　ㄉㄥㄦ",
      "definitions": [
        {
          "def": "把燈吹滅。"
        },
        {
          "def": "比喻交情破裂。",
          "example": [
            "如：「為了旅行的事，兩人吹燈兒了。」"
          ]
        },
        {
          "def": "比喻事情辦不成。",
          "example": [
            "如：「瞧！這兩個辦事員，只顧談他們的事情，把我們的事給吹燈兒了！」"
          ]
        },
        {
          "def": "比喻生命的終止。",
          "example": [
            "如：「王老伯已到燈盡油枯的邊緣了，看樣子也快吹燈兒了。」"
          ]
        }
      ],
      "pinyin": "chuī dēngr"
    }
  ],
  "title": "吹燈兒"
}