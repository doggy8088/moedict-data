      "bopomofo": "ㄩㄝˋ",
      "definitions": [
        {
          "def": "愉快、高興。",
          "example": [
            "如：「和顏悅色」。"
          ],
          "quote": [
            "《喻世明言．卷四○．沈小霞相會出師表》：「訪知其事，心中甚是不悅。」"
          ],
          "type": "形"
        },
        {
          "def": "使愉快。",
          "example": [
            "如：「悅耳」、「賞心悅目」。"
          ],
          "type": "動"
        },
        {
          "def": "喜愛。",
          "quote": [
            "《陳書．卷一．高祖本紀上》：「出為長城令，悅其山水，遂家焉。」"
          ],
          "type": "動"
        },
        {
          "def": "服從。",
          "example": [
            "如：「心悅誠服」。"
          ],
          "quote": [
            "《書經．武成》：「大賚于四海，而萬姓悅服。」"
          ],
          "type": "動"
        },
        {
          "def": "姓。如漢代有悅綰。",
          "type": "名"
        }
      ],
      "pinyin": "yuè"
    }
  ],
