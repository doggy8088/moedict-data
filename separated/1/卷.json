{
  "heteronyms": [
    {
      "bopomofo": "ㄐㄩㄢˋ",
      "definitions": [
        {
          "def": "古時書籍寫在絹帛或紙上，捲起來收藏，因此通稱書籍為「卷」。",
          "example": [
            "如：「手不釋卷」、「開卷有益」。"
          ],
          "type": "名"
        },
        {
          "def": "指書有分卷的每一單獨部分。",
          "example": [
            "如：「卷一」、「卷二」、「第一卷」、「第二卷」。"
          ],
          "type": "名"
        },
        {
          "def": "可以捲起來的書、畫。",
          "example": [
            "如：「經卷」、「手卷」、「畫卷」、「山水長卷」。"
          ],
          "type": "名"
        },
        {
          "def": "公私機構的公事文件。",
          "example": [
            "如：「卷宗」、「案卷」、「文卷」、「調卷」。"
          ],
          "type": "名"
        },
        {
          "def": "考試測驗的題紙。",
          "example": [
            "如：「試卷」、「考卷」。"
          ],
          "type": "名"
        },
        {
          "def": "量詞。計算字畫書籍的單位。",
          "example": [
            "如：「藏書三萬卷。」"
          ],
          "quote": [
            "唐．杜甫〈贈韋左丞丈二十二韻〉：「讀書破萬卷，下筆如有神。」"
          ],
          "type": "名"
        }
      ],
      "pinyin": "juàn"
    },
    {
      "bopomofo": "ㄑㄩㄢˊ",
      "definitions": [
        {
          "def": "彎曲的。",
          "example": [
            "如：「卷枝」、「卷雲」、「卷曲」。"
          ],
          "type": "形"
        },
        {
          "def": "好。",
          "quote": [
            "《詩經．陳風．澤陂》：「有美一人，碩大且卷。」",
            "漢．毛亨．傳：「卷，好貌。」"
          ],
          "type": "形"
        }
      ],
      "pinyin": "quán"
    },
    {
      "bopomofo": "ㄐㄩㄢˇ",
      "definitions": [
        {
          "def": "把質地軟的物品裹曲。",
          "example": [
            "如：「卷起袖子幹活。」、「把這畫卷起來。」"
          ],
          "link": [
            "通「捲」。"
          ],
          "type": "動"
        },
        {
          "def": "收藏。",
          "example": [
            "如：「卷藏」。"
          ],
          "link": [
            "通「捲」。"
          ],
          "quote": [
            "唐．柳宗元〈梓人傳〉：「則卷其術，默其智，悠爾而去。」"
          ],
          "type": "動"
        },
        {
          "def": "形狀卷曲的東西。",
          "example": [
            "如：「花卷」、「麻花卷」、「銀絲卷」。"
          ],
          "link": [
            "通「捲」。"
          ],
          "type": "名"
        },
        {
          "def": "可產生卷曲效果的器物。",
          "example": [
            "如：「髮卷」。"
          ],
          "type": "名"
        },
        {
          "def": "量詞。計算成卷物品的單位。",
          "example": [
            "如：「兩卷底片」、「三卷衛生紙」。"
          ],
          "type": "名"
        }
      ],
      "pinyin": "juǎn"
    }
  ],
