{
  "heteronyms": [
    {
      "bopomofo": "ㄔ",
      "definitions": [
        {
          "def": "口中咀嚼食物後嚥下。有時吸、飲液體也稱為「吃」。",
          "example": [
            "如：「吃飯」、「吃茶」、「吃藥」。"
          ],
          "type": "動"
        },
        {
          "def": "吸入、吸收。",
          "example": [
            "如：「吃煙」、「吃墨」。"
          ],
          "type": "動"
        },
        {
          "def": "下棋、玩牌時奪取對方的棋子或牌張。",
          "example": [
            "如：「抽車吃炮」、「吃掉老Ｋ」。"
          ],
          "type": "動"
        },
        {
          "def": "吞沒。",
          "example": [
            "如：「這貪官不知吃了多少民脂民膏！」、「我的儲蓄全給他吃了！」"
          ],
          "type": "動"
        },
        {
          "def": "沉入。用來說明船舶下沉於水面下程度的用語。",
          "example": [
            "如：「這船吃水多深？」"
          ],
          "type": "動"
        },
        {
          "def": "擔負。",
          "example": [
            "如：「吃重」、「吃不住」、「吃不消」。"
          ],
          "type": "動"
        },
        {
          "def": "捱、受。",
          "example": [
            "如：「吃驚」、「吃虧」、「吃官司」、「吃得苦中苦，方為人上人。」"
          ],
          "type": "動"
        },
        {
          "def": "理會、接納。",
          "example": [
            "如：「他軟硬不吃！」、「誰吃這一套！」"
          ],
          "type": "動"
        },
        {
          "def": "耗費。",
          "example": [
            "如：「吃力」。"
          ],
          "type": "動"
        }
      ],
      "pinyin": "chī"
    },
    {
      "bopomofo": "ㄐㄧˊ",
      "definitions": [
        {
          "def": "說話結巴、言語困難的樣子。",
          "example": [
            "如：「口吃」。"
          ],
          "quote": [
            "《說文解字．口部》：「吃，言蹇難也。」"
          ],
          "type": "形"
        },
        {
          "def": "形容笑聲。",
          "quote": [
            "漢．伶玄《趙飛燕外傳》：「帝昏夜擁昭儀居七成帳，笑吃吃不止。」"
          ],
          "type": "狀"
        }
      ],
      "pinyin": "jí"
    }
  ],
  "non_radical_stroke_count": 3,
  "radical": "口",
  "stroke_count": 6,
  "title": "吃"
}