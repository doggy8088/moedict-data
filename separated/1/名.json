      "definitions": [
        {
          "def": "人的稱號。",
          "example": [
            "如：「尊姓大名」、「請問芳名」。"
          ],
          "type": "名"
        },
        {
          "def": "事物的稱號。",
          "example": [
            "如：「地名」。"
          ],
          "quote": [
            "《管子．心術上》：「物固有形、形固有名。」"
          ],
          "type": "名"
        },
        {
          "def": "聲譽。",
          "example": [
            "如：「盛名」、「令名」。"
          ],
          "type": "名"
        },
        {
          "def": "表示概念的名稱。",
          "quote": [
            "《老子．第一章》：「名可名，非常名。」",
            "《荀子．正名》：「名無固宜，約之以命，約定俗成，謂之宜。」"
          ],
          "type": "名"
        },
        {
          "def": "量詞：(1) 計算人的單位。如：「學生十名，缺席一名。」(2) 計算排名的單位。如：「全班第十名。」",
          "type": "名"
        },
        {
          "def": "指稱、形容。",
          "example": [
            "如：「莫名其妙」、「無以名之」。"
          ],
          "type": "動"
        },
        {
          "def": "有名的。",
          "example": [
            "如：「名人」、「名馬」。"
          ],
          "type": "形"
        },
        {
          "def": "貴重的、出色的。",
          "example": [
            "如：「名器」、「名酒」。"
          ],
          "type": "形"
        }
      ],
      "pinyin": "míng"
    }
  ],
