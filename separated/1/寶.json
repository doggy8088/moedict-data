      "definitions": [
        {
          "def": "珍貴的東西。",
          "example": [
            "如：「珠寶」、「國寶」、「傳家之寶」。"
          ],
          "quote": [
            "《禮記．禮運》：「故天不愛其道，地不愛其寶，人不愛其情。」"
          ],
          "type": "名"
        },
        {
          "def": "舊時錢幣。",
          "example": [
            "如：「元寶」、「通寶」。"
          ],
          "type": "名"
        },
        {
          "def": "璽印。舊時天子御璽、諸侯封璽都稱「寶」。",
          "quote": [
            "《詩經．大雅．崧高》：「錫爾介圭，以作爾寶。」"
          ],
          "type": "名"
        },
        {
          "def": "姓。漢代有寶忠，元代有寶魯。",
          "type": "名"
        },
        {
          "def": "珍貴的。",
          "example": [
            "如：「寶刀」、「寶劍」。"
          ],
          "type": "形"
        },
        {
          "def": "稱君主或神、道、佛有關事物。",
          "example": [
            "如：「寶位」、「寶符」、「寶塔」、「寶剎」。"
          ],
          "type": "形"
        },
        {
          "def": "尊稱他人。",
          "example": [
            "如：「寶眷」、「貴寶號」。"
          ],
          "type": "形"
        }
      ],
      "pinyin": "bǎo"
    }
