  "heteronyms": [
    {
      "bopomofo": "ㄉㄤˋ",
      "definitions": [
        {
          "def": "有紋理的石頭。",
          "quote": [
            "《文選．何晏．景福殿賦》：「墉垣碭基，其光昭昭。」",
            "李善注引《說文》曰：「碭，文石也。」"
          ],
          "type": "名"
        },
        {
          "def": "山名。即碭山。在大陸地區安徽省碭山縣東南。",
          "type": "名"
        },
        {
          "def": "姓。如漢代有碭魯賜。",
          "type": "名"
        },
        {
          "def": "振盪。",
          "quote": [
            "《莊子．庚桑楚》：「吞舟之魚，碭而失水，則蟻能苦之。」"
          ],
          "type": "動"
        },
        {
          "def": "大、廣。",
          "quote": [
            "《淮南子．本經》：「當此之時，玄玄至碭而運照。」",
            "漢．高誘．注：「碭，大也。盛德之君，恩仁廣大，遍照四海也。」"
          ],
          "type": "形"
        }
      ],
      "pinyin": "dàng"
    }
  ],
