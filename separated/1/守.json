      "definitions": [
        {
          "def": "官職、職責。",
          "quote": [
            "《漢書．卷三○．藝文志》：「墨家者流，蓋出於清廟之守。」",
            "元．關漢卿《謝天香．第四折》：「官守所拘，功名在念，豈飲酒。」"
          ],
          "type": "名"
        },
        {
          "def": "職官名。古代一郡的行政長官。",
          "example": [
            "如：「郡守」、「太守」。"
          ],
          "type": "名"
        },
        {
          "def": "節操、品性。",
          "example": [
            "如：「操守」。"
          ],
          "quote": [
            "《禮記．儒行》：「劫之以眾，沮之以兵，見死不更其守。」"
          ],
          "type": "名"
        },
        {
          "def": "姓。如宋代有守恭。",
          "type": "名"
        },
        {
          "def": "防衛、保護。",
          "example": [
            "如：「防守」、「駐守」、「堅守」。"
          ],
          "type": "動"
        },
        {
          "def": "保持、堅持。",
          "example": [
            "如：「守信」、「守節」、「墨守成規」、「守身如玉」。"
          ],
          "type": "動"
        },
        {
          "def": "遵行。",
          "example": [
            "如：「恪守」、「守法」、「遵守」、「守規律」。"
          ],
          "type": "動"
        },
        {
          "def": "等待。",
          "example": [
            "如：「守候」、「獨守」、「守歲」、「空守一夜」。"
          ],
          "type": "動"
        },
        {
          "def": "依賴、依傍。",
          "quote": [
            "《水滸傳．第一八回》：「自和他父親宋太公在村中務農，守些田園過活。」"
          ],
          "type": "動"
        }
