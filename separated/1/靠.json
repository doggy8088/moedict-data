  "heteronyms": [
    {
      "bopomofo": "ㄎㄠˋ",
      "definitions": [
        {
          "def": "相違背。",
          "quote": [
            "《說文解字．非部》：「靠，相韋也。」",
            "清．段玉裁．注：「今俗謂相依曰靠，古人謂相背曰靠。」"
          ],
          "type": "動"
        },
        {
          "def": "倚傍。",
          "example": [
            "如：「靠牆」、「倚靠」、「靠著大樹」。"
          ],
          "type": "動"
        },
        {
          "def": "挨近、貼近。",
          "example": [
            "如：「靠岸」、「靠著站」。"
          ],
          "quote": [
            "唐．曹松〈宿溪僧院〉詩：「煎茶留靜者，靠月坐蒼山。」"
          ],
          "type": "動"
        },
        {
          "def": "仰賴、依憑。",
          "example": [
            "如：「靠實力」、「靠天吃飯」、「靠人幫忙」。"
          ],
          "type": "動"
        },
        {
          "def": "信賴、信任。",
          "example": [
            "如：「可靠」、「靠不住」。"
          ],
          "quote": [
            "《老殘遊記．第一六回》：「這是同裕的票子，是敝縣第一個大錢莊，萬靠得住。」"
          ],
          "type": "動"
        },
        {
          "def": "車、船的停駐、停泊。",
          "example": [
            "如：「停靠」、「靠站」、「靠岸」。"
          ],
          "type": "動"
        },
        {
          "def": "古代戲劇中武將所穿的鎧甲。",
          "example": [
            "如：「靠子」、「女靠」、「軟靠」。"
          ],
          "quote": [
            "《儒林外史．第四二回》：「那小戲子一個個戴了貂裘，簪了雉羽，穿極新鮮的靠子，跑上場來，串了　一個五花八門。」"
          ],
          "type": "名"
        }
      ],
      "pinyin": "kào"
    }
  ],
