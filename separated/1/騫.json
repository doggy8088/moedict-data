  "heteronyms": [
    {
      "bopomofo": "ㄑㄧㄢ",
      "definitions": [
        {
          "def": "虧損、損壞。",
          "quote": [
            "《詩經．小雅．天保》：「如南山之壽，不騫不崩。」"
          ],
          "type": "動"
        },
        {
          "def": "高舉。",
          "quote": [
            "唐．李華〈含元殿賦〉：「進而仰之，騫龍首而張鳳翼。」"
          ],
          "type": "動"
        },
        {
          "def": "飛起。",
          "quote": [
            "唐．杜甫〈寄岳州賈司馬六丈巴州嚴八使君兩閣老五十韻〉：「如公盡雄俊，志在必騰騫。」"
          ],
          "type": "動"
        },
        {
          "def": "拔取。",
          "link": [
            "通「搴」。"
          ],
          "quote": [
            "《漢書．卷九○．酷吏傳．楊樸傳》：「將軍之功，獨有先破石門、尋陿，非有斬將騫旗之實也。」"
          ],
          "type": "動"
        },
        {
          "def": "錯過、延誤。",
          "link": [
            "通「愆」。"
          ],
          "quote": [
            "《文選．劉琨．扶風歌》：「惟昔李騫期，寄在匈奴庭。」"
          ],
          "type": "動"
        },
        {
          "def": "過錯。",
          "link": [
            "通「愆」。"
          ],
          "quote": [
            "《荀子．正名》：「長夜漫兮，永思騫兮。」"
          ],
          "type": "名"
        },
        {
          "def": "姓。如漢代有騫包。",
          "type": "名"
        }
      ],
      "pinyin": "qiān"
    },
