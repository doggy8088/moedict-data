    {
      "bopomofo": "ㄆㄠˊ",
      "definitions": [
        {
          "def": "燒、烤。",
          "example": [
            "如：「炮烙」、「烹羊炮羔」。"
          ],
          "quote": [
            "《詩經．小雅．瓠葉》：「有兔斯首，炮之燔之。」",
            "《左傳．昭公二十七年》：「令尹炮之，盡滅郤氏之族黨。」"
          ],
          "type": "動"
        },
        {
          "def": "以火烘焙精煉藥材。",
          "example": [
            "如：「炮煉」、「炮煎」。"
          ],
          "type": "動"
        }
      ],
      "pinyin": "páo"
    },
    {
      "bopomofo": "ㄅㄠ",
      "definitions": [
        {
          "def": "一種烹飪法。在旺火上急炒。用於快炒肉片、肉丁等。",
          "example": [
            "如：「炮羊肉」、「炮炒雞丁」。"
          ],
          "type": "動"
        },
        {
          "def": "把東西置於器物上烘乾。",
          "example": [
            "如：「將鞋子放在爐臺上炮乾。」"
          ],
          "type": "動"
        }
      ],
      "pinyin": "bāo"
    },
    {
      "bopomofo": "ㄆㄠˋ",
      "definitions": [
        {
          "def": "武器名。一種發射鐵石彈丸或炮彈的重型兵器。火力強，射程遠，極具殺傷力和破壞力。",
          "example": [
            "如：「大炮」、「開炮」、「高射炮」、「迫擊炮」。"
          ],
          "type": "名"
        },
        {
          "def": "爆竹。",
          "example": [
            "如：「鞭炮」。"
          ],
          "type": "名"
        }
      ],
      "pinyin": "pào"
    }
  ],
