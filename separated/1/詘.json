    {
      "bopomofo": "ㄑㄩ",
      "definitions": [
        {
          "def": "彎曲。",
          "link": [
            "同「屈」。"
          ],
          "quote": [
            "《荀子．勸學》：「若挈裘領，詘五指而頓之，順者不可勝數也。」"
          ],
          "type": "動"
        },
        {
          "def": "屈服、折服。",
          "quote": [
            "《戰國策．秦策一》：「今欲并天下，凌萬乘，詘敵國，制海內，子元元，臣諸侯，非兵不可。」"
          ],
          "type": "動"
        },
        {
          "def": "吐詞艱澀、言語鈍拙。",
          "link": [
            "通「訥」。"
          ],
          "quote": [
            "《史記．卷一○三．萬石張叔傳．太史公曰》「仲尼有言：『君子欲訥於言』」句下裴駰集解引徐廣曰：「『訥』字多作『詘』，音同耳。古字假借。」"
          ],
          "type": "動"
        },
        {
          "def": "冤屈。",
          "quote": [
            "《呂氏春秋．貴直論．壅塞》：「宋王因怒而詘殺之。」"
          ],
          "type": "副"
        },
        {
          "def": "反而。",
          "quote": [
            "《戰國策．秦策四》：「一舉眾而注地於楚，詘令韓魏歸帝重於齊，是王失計也。」"
          ],
          "type": "副"
        },
        {
          "def": "姓。如漢代有詘強。",
          "type": "名"
        }
      ],
      "pinyin": "qū"
    },
    {
      "bopomofo": "ㄔㄨˋ",
      "definitions": [
        {
          "def": "貶低。",
          "link": [
            "同「黜」。"
          ],
          "quote": [
            "《戰國策．韓策三》：「彼公仲者，秦勢能詘之。」"
          ],
          "type": "動"
        }
      ],
      "pinyin": "chù"
    }
