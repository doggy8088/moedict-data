      "definitions": [
        {
          "def": "趴，身體向前傾靠在物體上。",
          "example": [
            "如：「倒伏」、「伏案」。"
          ],
          "quote": [
            "《禮記．曲禮上》：「坐毋箕，寢毋伏。」"
          ],
          "type": "動"
        },
        {
          "def": "隱藏、隱匿。",
          "example": [
            "如：「蟄伏」、「危機四伏」、「晝伏夜出」。"
          ],
          "quote": [
            "《老子．第五八章》：「禍兮福之所倚，福兮禍之所伏。」"
          ],
          "type": "動"
        },
        {
          "def": "降服、使屈服。",
          "example": [
            "如：「降龍伏虎」、「制伏」。"
          ],
          "type": "動"
        },
        {
          "def": "低落下去。",
          "example": [
            "如：「伏地挺身」、「此起彼伏」。"
          ],
          "type": "動"
        },
        {
          "def": "承認、承受。",
          "example": [
            "如：「伏罪」、「伏法」。"
          ],
          "quote": [
            "《文選．班彪．王命論》：「遇折足之凶，伏斧鉞之誅。」"
          ],
          "type": "動"
        },
        {
          "def": "佩服、信服。",
          "link": [
            "同「服」。"
          ],
          "quote": [
            "唐．白居易〈琵琶行〉：「曲罷曾教善才伏，妝成每被秋娘妒。」"
          ],
          "type": "動"
        },
        {
          "def": "隱藏不露的。",
          "quote": [
            "《文選．左思．蜀都賦》：「漏江伏流潰其阿。」"
          ],
          "type": "形"
        },
        {
          "def": "參見「伏日」條。",
          "type": "名"
        },
        {
          "def": "對尊長自謙的敬辭。",
          "quote": [
            "《文選．枚乘．七發》：「伏聞太子玉體不安，亦少閒乎？」",
            "《漢書．卷四．文帝紀》：「臣伏計之，大王奉高祖宗廟最宜稱。」"
          ],
          "type": "副"
        }
