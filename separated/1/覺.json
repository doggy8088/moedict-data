  "heteronyms": [
    {
      "bopomofo": "ㄐㄩㄝˊ",
      "definitions": [
        {
          "def": "睡醒。",
          "quote": [
            "《詩經．王風．兔爰》：「我生之後，逢此百憂，尚寐無覺。」",
            "《莊子．齊物論》：「覺而後知其夢也。」"
          ],
          "type": "動"
        },
        {
          "def": "醒悟、感悟。",
          "example": [
            "如：「覺悟」、「後知後覺」。"
          ],
          "quote": [
            "《文選．陶淵明．歸去來辭》：「寔迷途其未遠，覺今是而昨非。」"
          ],
          "type": "動"
        },
        {
          "def": "知曉、感受到、意識到。",
          "example": [
            "如：「發覺」、「警覺」、「自覺」、「不知不覺」。"
          ],
          "quote": [
            "唐．李商隱〈無題〉詩：「曉鏡但愁雲鬢改，夜吟應覺月光寒。」"
          ],
          "type": "動"
        },
        {
          "def": "啟發、告訴。",
          "quote": [
            "《孟子．萬章上》：「使先知覺後知。」"
          ],
          "type": "動"
        },
        {
          "def": "感官受刺激後對事物的辨識能力。",
          "example": [
            "如：「知覺」、「味覺」、「幻覺」、「錯覺」。"
          ],
          "type": "名"
        },
        {
          "def": "賢智之人。",
          "example": [
            "如：「先覺」。"
          ],
          "type": "名"
        }
      ],
      "pinyin": "jué"
    },
    {
      "bopomofo": "ㄐㄧㄠˋ",
      "definitions": [
        {
          "def": "睡眠。",
          "example": [
            "如：「睡午覺」、「睡大覺」。"
          ],
          "type": "名"
        },
        {
          "def": "量詞。計算入睡次數的單位。",
          "example": [
            "如：「睡了一覺」。"
          ],
          "type": "名"
        }
      ],
