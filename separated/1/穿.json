{
  "heteronyms": [
    {
      "bopomofo": "ㄔㄨㄢ",
      "definitions": [
        {
          "def": "挖鑿、挖掘。",
          "quote": [
            "《詩經．召南．行露》：「誰謂鼠無牙，何以穿我墉？」",
            "《禮記．月令》：「穿竇窖，脩囷倉。」"
          ],
          "type": "動"
        },
        {
          "def": "貫通、通過。",
          "example": [
            "如：「穿針引線」、「穿鞋帶」、「穿越馬路」。"
          ],
          "type": "動"
        },
        {
          "def": "著衣襪等。",
          "example": [
            "如：「穿衣服」、「穿襪子」。"
          ],
          "type": "動"
        },
        {
          "def": "破敗的。",
          "quote": [
            "《莊子．山木》：「衣弊屐穿，貧也，非憊也。」"
          ],
          "type": "形"
        },
        {
          "def": "透澈、明白、顯明。",
          "example": [
            "如：「說穿」、「看穿心事」、「望穿秋水」、「拆穿西洋鏡」。"
          ],
          "type": "副"
        }
      ],
      "pinyin": "chuān"
    }
  ],
  "non_radical_stroke_count": 4,
  "radical": "穴",
  "stroke_count": 9,
  "title": "穿"
}