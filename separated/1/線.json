  "heteronyms": [
    {
      "bopomofo": "ㄒㄧㄢˋ",
      "definitions": [
        {
          "def": "用棉、麻、絲、金屬等製成的細縷或細長物，可隨意彎折。",
          "example": [
            "如：「毛線」、「電線」、「穿針引線」。"
          ],
          "type": "名"
        },
        {
          "def": "比喻像線一般細長的東西。",
          "example": [
            "如：「光線」、「射線」。"
          ],
          "type": "名"
        },
        {
          "def": "幾何學上指一點任意移動而成的圖形。是「面」的界，有位置與長度，無厚度與高度。",
          "example": [
            "如：「直線」、「曲線」、「折線」。"
          ],
          "type": "名"
        },
        {
          "def": "交通路徑。",
          "example": [
            "如：「航線」、「路線」、「縱貫線」、「運輸線」。"
          ],
          "type": "名"
        },
        {
          "def": "邊緣、邊界。",
          "example": [
            "如：「前線」、「防線」、「海岸線」。"
          ],
          "type": "名"
        },
        {
          "def": "比喻將要接近的某種界限。",
          "example": [
            "如：「死亡線」、「生命線」。"
          ],
          "type": "名"
        },
        {
          "def": "祕密尋求關於人、事、物的門路。",
          "example": [
            "如：「眼線」、「內線」、「線索」。"
          ],
          "quote": [
            "《儒林外史．第三四回》：「他原是賊頭趙大一路做線的。」"
          ],
          "type": "名"
        },
        {
          "def": "量詞。計算線路數量的單位。",
          "example": [
            "如：「本公司共有五線電話。」"
          ],
          "type": "名"
        }
      ],
      "pinyin": "xiàn"
    }
