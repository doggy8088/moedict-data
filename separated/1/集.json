  "heteronyms": [
    {
      "bopomofo": "ㄐㄧˊ",
      "definitions": [
        {
          "def": "棲息。",
          "quote": [
            "《詩經．唐風．鴇羽》：「肅肅鴇羽，集于苞栩。」",
            "南朝宋．鮑照〈贈傅都曹別詩〉：「輕鴻戲江潭，孤鴈集洲沚。」"
          ],
          "type": "動"
        },
        {
          "def": "把散開的事物聚合在一起。",
          "example": [
            "如：「集合」、「聚集」、「籌集」、「集資」、「收集」。"
          ],
          "quote": [
            "《文選．賈誼．過秦論》：「天下雲集響應，嬴糧而景從。」"
          ],
          "type": "動"
        },
        {
          "def": "市場、人口聚集舉行交易的地方。",
          "example": [
            "如：「市集」、「趕集」、「年集」。"
          ],
          "type": "名"
        },
        {
          "def": "地名用字。",
          "example": [
            "如：「張家集」。"
          ],
          "quote": [
            "《儒林外史．第二回》：「話說山東兗州府汶上縣有個鄉村，叫做薛家集。」"
          ],
          "type": "名"
        },
        {
          "def": "輯錄詩文等的書籍。",
          "example": [
            "如：「詩集」、「文集」。"
          ],
          "quote": [
            "《文選．曹丕．與吳質書》：「頃撰其遺文，都為一集。」"
          ],
          "type": "名"
        },
        {
          "def": "量詞。計算叢書或影劇集系列的單位。",
          "example": [
            "如：「這齣戲今天播出第三集。」"
          ],
          "type": "名"
        },
        {
          "def": "中國傳統圖書四部分類的第四部，包括詩文等的作品稱為「集」。",
          "type": "名"
        }
      ],
      "pinyin": "jí"
    }
  ],
