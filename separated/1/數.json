      "pinyin": "sù"
    },
    {
      "bopomofo": "ㄘㄨˋ",
      "definitions": [
        {
          "def": "細密。",
          "quote": [
            "《孟子．梁惠王上》：「數罟不入洿池，魚鱉不可勝食也。」"
          ],
          "type": "形"
        }
      ],
      "pinyin": "cù"
    }
